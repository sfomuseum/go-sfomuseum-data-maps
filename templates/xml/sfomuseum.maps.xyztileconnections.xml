{{ define "qgis_tile_connections" -}}
<!DOCTYPE connections>
<!--
    This file was generated by the go-sfomuseum-maps package on {{ .LastModified }}:
    {{ .Args }}

    Important: The "qgsXYZTilesConnections" document definition does not provide any means to define
    extents for an XYZ layer so if you load any of these SFO aerial layers outside the general area
    of the San Francisco Internation Airport QGIS will start emitting a lot of tile requests that will
    fail.
-->
<qgsXYZTilesConnections version="1.0">
  {{ range $t := .TileConnections -}}
    <xyztiles username="{{ $t.Username  }}" referer="{{ $t.Referer }}" zmax="{{ $t.ZMax }}" password="{{ $t.Password }}" tilePixelRatio="{{ $t.TilePixelRatio }}" url="{{ $t.URL }}" name="{{ $t.Name }}" authcfg="{{ $t.AuthConfig }}" http-header:referer="{{ $t.Referer }}" zmin="{{ $t.ZMin }}"/>
  {{ end -}}
</qgsXYZTilesConnections>
{{ end }}
